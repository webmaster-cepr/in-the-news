<?xml version="1.0" encoding="utf-8"?><form>        <fieldset>                				<field                        name="id"                        type="text"						readonly="true"						class="readonly"                />                				<field                        name="title"                        type="text"                        label="Headline"                        description="Headline of article"                        size="75"                        class="inputbox"                        default=""                />                <field                        name="url"                        type="text"                        label="URL"                        description="URL of article"                        size="75"                        class="inputbox"                        default=""                />									            <field                        name="publication"                        type="text"                        label="Publication"                        description="Where the article was published"                        size="75"                        class="inputbox"                        default=""                />												<field 						name="italicized" 						type="checkboxes"						label="Italicize Pub Name"						description="Print publications are italicized"						default="0"						>					<option value="0">No</option>					<option value="1">Yes</option>				</field>				                <field                        name="published"                        type="calendar"                        label="Date published"                        description="Date of article"                        default=""						format="%Y-%m-%d"                />					                <field                        name="source"                        type="text"						label="Expert"                        description="Expert mentioned in the article"                        size="75"                        class="inputbox"                        default=""                />					<field name="type" type="list" default="" label="Type of press hit" description="">					<option value="News">News</option>					<option value="Blogs">Blogs</option>					<option value="TV/Video">TV/Video</option>					<option value="Radio Interview">Radio Interview</option>				</field>								<field name="language" type="list" default="" label="Language" description="">					<option value="English">English</option>					<option value="Spanish">Spanish</option>					<option value="Portuguese">Portuguese</option>					<option value="Bulgarian">Bulgarian</option>					<option value="Catalan">Catalan</option>					<option value="Chinese">Chinese</option>					<option value="Czech">Czech</option>					<option value="Danish">Danish</option>					<option value="Dutch">Dutch</option>					<option value="Finnish">Finnish</option>					<option value="French">French</option>					<option value="Galician">Galician</option>					<option value="German">German</option>					<option value="Greek">Greek</option>					<option value="Hungarian">Hungarian</option>					<option value="Italian">Italian</option>					<option value="Korean">Korean</option>					<option value="Latvian">Latvian</option>					<option value="Norwegian">Norwegian</option>					<option value="Polish">Polish</option>					<option value="Romanian">Romanian</option>					<option value="Russian">Russian</option>					<option value="Slovenian">Slovenian</option>					<option value="Swedish">Swedish</option>					<option value="Thai">Thai</option>					<option value="Vietnamese">Vietnamese</option>				</field>				<field 						name="frontpage" 						type="checkboxes"						label="Frontpage"						description="Posts hit on frontpage of site"						default="0"						>					<option value="0">No</option>					<option value="1">Yes</option>				</field>					<field name="live" type="list" default="" label="Post to Website" description="">					<option value="Yes">Yes</option>					<option value="No">No</option>				</field>					<field 						name="copy" 						type="checkboxes"						label="Copy"						description="Copy of another press hit?"						default="0"						>					<option value="0">No</option>					<option value="1">Yes</option>				</field>								<field name="team" type="list" default="" label="Team" description="">					<option value="Domestic">Domestic</option>					<option value="International">International</option>					<option value="Both">Both</option>				</field>                <field                        name="subject"                        type="text"						label="Subject"                        description="Subject of press hit"                        size="75"                        class="inputbox"                        default=""                />							</fieldset></form>